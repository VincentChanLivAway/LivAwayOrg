        .client
            img(src="assets/images/partners/10.png" alt="")
        .client
            img(src="assets/images/partners/7.png" alt="")

    .owl-carousel.clients-carousel(data-carousel-options='{"dots": false}')
        .client
            img(src="assets/images/partners/15.png" alt="")
        .client
            img(src="assets/images/partners/13.png" alt="")
        .client
            img(src="assets/images/partners/11.png" alt="")
        .client
            img(src="assets/images/partners/6.png" alt="")
        .client
            img(src="assets/images/partners/5.png" alt="")
        .client
            img(src="assets/images/partners/3.png" alt="")
        .client
            img(src="assets/images/partners/2.png" alt="")
            
    .owl-carousel.clients-carousel(data-carousel-options='{"dots": false}')
        .client
            img(src="assets/images/partners/9.png" alt="")
        .client
            img(src="assets/images/partners/12.png" alt="")
        .client
            img(src="assets/images/partners/16.png" alt="")
        .client
            img(src="assets/images/partners/17.png" alt="")
        .client
            img(src="assets/images/partners/21.png" alt="")
        .client
            img(src="assets/images/partners/4.png" alt="")
        .client
            img(src="assets/images/partners/19.png" alt="")
    // Partners end

    = "\n"

    // Form
    //section.module.divider-top#contact
        .container

            .row
                .col-lg-6.col-md-9.m-auto.text-center
                    h1 Let's talk.
                    p.lead See how we can help your startup to reach great success in Chinese market.

            .row
                .col-md-12
                    .space(data-MY="60px")

            .row
                .col-md-12
                    form#contact-form(method="post" novalidate)
                        .row
                            .col-md-4
                                .form-group
                                    input.form-control(type="text" name="name" placeholder="Name" required="")
                                    p.help-block.text-danger
                            .col-md-4
                                .form-group
                                    input.form-control(type="email" name="email" placeholder="E-mail" required="")
                                    p.help-block.text-danger
                            .col-md-4
                                .form-group
                                    input.form-control(type="text" name="subject" placeholder="Subject" required="")
                                    p.help-block.text-danger
                            .col-md-12
                                .form-group
                                    textarea.form-control(name="message" placeholder="Message" rows="12" required="")
                            .col-md-12
                                p
                                .text-center
                                    input(type="submit" class="btn btn-circle btn-brand" value="Send Message")
                    // Ajax response
                    #contact-response.ajax-response.text-center
    // Form end

    = "\n"